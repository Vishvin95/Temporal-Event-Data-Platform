<?xml version="1.0" encoding="UTF-8"?>

<scenario name="Company">
	<domains>
		<domain name="employee">
			<temporal>true</temporal>
			<event>
				<name>empName</name>
				<dataType>string</dataType>
			</event>
			<event>
				<name>salary</name>
				<dataType>integer</dataType>
				<notnull>true</notnull>
				<unique>true</unique>
			</event>
		</domain>
		<domain name="project">
			<temporal>false</temporal>
			<event>
				<name>projectName</name>
				<dataType>string</dataType>
			</event>
			<event>
				<name>cost</name>
				<dataType>integer</dataType>
			</event>
		</domain>
		<domain name="dependent">
			<temporal>true</temporal>
			<event>
				<name>dependentName</name>
				<dataType>string</dataType>
			</event>
			<event>
				<name>dob</name>
				<dataType>date</dataType>
			</event>
		</domain>
	</domains>
	<relationships>
		<relationship name="works_on">
			<type>nn</type>
			<from>employee</from>
			<to>project</to>
		</relationship>
		<relationship name="has">
			<type>1n</type>
			<from>employee</from>
			<to>dependent</to>
		</relationship>
	</relationships>
</scenario>